<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mapper.HotelMapper">

  <select id="findByKeyword" parameterType="string" resultType="com.example.demo.model.Hotel">
    SELECT * FROM hotel WHERE prefecture LIKE CONCAT('%', #{keyword}, '%')
  </select>

</mapper>
